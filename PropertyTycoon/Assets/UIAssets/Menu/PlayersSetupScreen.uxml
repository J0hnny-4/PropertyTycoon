        </engine:VisualElement>
        <engine:VisualElement name="footer" style="align-items: flex-start; justify-content: space-around;">
            <engine:Button text="BACK" name="back-button" class="simple"/>
            <engine:Button text="CONTINUE" name="ready-button" class="simple"/>
        </engine:VisualElement>
    </engine:VisualElement>
